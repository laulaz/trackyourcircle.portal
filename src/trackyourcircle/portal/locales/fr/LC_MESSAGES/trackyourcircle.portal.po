#: trackyourcircle.portal:/browser/circle.py:160
msgid "Add Circle"
msgstr "Ajouter un cercle"

#: trackyourcircle.portal:/browser/add-circle-help.pt:2
msgid "Add missing Circle"
msgstr "Ajouter un cercle manquant"

#: trackyourcircle.portal:/browser/circle.py:204
msgid "Cancel"
msgstr "Annuler"

#: trackyourcircle.portal:/browser/search-circle-help.pt:4
msgid "First, search if the circle already exists"
msgstr "1) Chercher si le cercle a déjà été ajouté."

#: trackyourcircle.portal:/browser/add-circle-help.pt:6
msgid "Fourth, save the circle."
msgstr "4) Enregistrer"

#: trackyourcircle.portal:/browser/add-circle-help.pt:5
msgid "Or search again."
msgstr "Ou chercher encore"

#: trackyourcircle.portal:/browser/circle.py:230
msgid "Save"
msgstr "Enregistrer"

#: trackyourcircle.portal:/browser/circle.py:186
msgid "Search"
msgstr "Chercher"

#: trackyourcircle.portal:/browser/circle-search.pt:9
msgid "Search Circle"
msgstr "Chercher votre cercle"

#: trackyourcircle.portal:/browser/search-circle-help.pt:2
msgid "Search and add Circle"
msgstr "Chercher et ajouter un cercle"

#: trackyourcircle.portal:/browser/search-circle-help.pt:5
msgid "Second, if not, add the missing circle."
msgstr "2) S'il n'existe pas, ajouter le cercle manquant."

#: trackyourcircle.portal:/browser/circle.py:58
msgid "Specify city, village or municipality."
msgstr "Indiquer la ville, le village ou la commune."

#: trackyourcircle.portal:/browser/circle.py:47
msgid "Specify location like Times Square"
msgstr "Indiquer le lieu. Par exemple, Place communale."

#: trackyourcircle.portal:/browser/view-circle-help.pt:3
msgid "There are ${circle_number} circles."
msgstr "Il y a ${circle_number} cercles."

#: trackyourcircle.portal:/browser/add-circle-help.pt:4
msgid "Third, id needed, move the red marker on the map to the precise location."
msgstr "3) Si nécessaire, déplacer le marqueur rouge vers l'endroit exact."

#. Default: "Click on the map to select a location, or use the text input to search by address."
#: trackyourcircle.portal:/browser/circle.py:72
msgid "help_geolocation"
msgstr ""

#. Default: "City"
#: trackyourcircle.portal:/browser/circle.py:38
msgid "label_city"
msgstr "Ville"

#. Default: "Geolocation"
#: trackyourcircle.portal:/browser/circle.py:71
msgid "label_geolocation"
msgstr "Géolocalisation"

#. Default: "Location"
#: trackyourcircle.portal:/browser/circle.py:42
msgid "label_location"
msgstr "Lieu"
